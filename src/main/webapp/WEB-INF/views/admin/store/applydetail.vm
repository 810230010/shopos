#parse("/admin/models/begin.vm")
<link href="/assets/admin/css/plugins/select2/select2.min.css" rel="stylesheet">
<link href="/assets/admin/css/plugins/touchspin/jquery.bootstrap-touchspin.min.css" rel="stylesheet">
<link href="/assets/admin/css/plugins/magiccheck/magic-check.css" rel="stylesheet" type="text/css">
<!-- 文件上传 -->
<link href="/assets/admin/css/plugins/fileinput/fileinput.min.css" media="all" rel="stylesheet" type="text/css"/>

#parse("/admin/models/header.vm")
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>商家-申请店铺详细信息</h2>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">
                    <h5><small>申请详情</small></h5>
                </div>
                <div class="ibox-content">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">店主用户名</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.memberName)
                                    $store.memberName
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">申请店铺名</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.storeName)
                                    $store.storeName
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">公司名</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.companyName)
                                    $store.companyName
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">公司详细地址</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.companyAddressDetail)
                                    $store.companyAddressDetail
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">公司电话</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.companyPhone)
                                    $store.companyPhone
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">注册总资金</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.companyRegisteredCapital)
                                    $store.companyRegisteredCapital
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">联系人姓名</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.contactsName)
                                    $store.contactsName
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">联系人电话</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.contactsPhone)
                                    $store.contactsPhone
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">营业执照号</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.businessLicenceNumber)
                                    $store.businessLicenceNumber
                                    #end
                                </span>
                            </div>
                        </div>
                        
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">法定经营范围</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.businessSphere)
                                    $store.businessSphere
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">营业执照电子版</label>
                            <div class="col-sm-3">
                                <img src="$store.businessLicenceNumberElectronic"/>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">税务登记证号</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.taxRegistrationCertificateElectronic)
                                    $store.taxRegistrationCertificateElectronic
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">店铺分类名称</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.scName)
                                    $store.scName
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">付款金额</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($store.payingAmount)
                                    $store.payingAmount
                                    #end
                                </span>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
#parse("/admin/models/footer.vm")
<!-- 文件上传 -->
<script src="/assets/admin/js/plugins/fileinput/fileinput.min.js" type="text/javascript"></script>
<script src="/assets/admin/js/plugins/fileinput/zh.js" type="text/javascript"></script>
<script src="/assets/admin/js/plugins/fileinput/theme.js" type="text/javascript"></script>

<!-- Jquery Validate -->
<script src="/assets/admin/js/plugins/validate/jquery.validate.min.js"></script>
#parse("/admin/models/end.vm")