#parse("/admin/models/header.vm")
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>申请店铺详细信息</h2>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">
                    <h5><small>申请详情</small></h5>
                </div>
                <div class="ibox-content">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">店主用户名</label>
                            <div class="col-sm-7">
                                <input type="text" value="$store.memberName" class="form-control" disabled>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">申请店铺名</label>
                            <div class="col-sm-7">
                                <input type="text" value="$store.storeName" class="form-control" disabled>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">公司名</label>
                            <div class="col-sm-7">
                                <input type="text" value="$store.companyName" class="form-control" disabled>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">公司详细地址</label>
                            <div class="col-sm-7">
                                <input value="$store.companyAddressDetail" name="sort" class="form-control" type="text" disabled/>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">公司电话</label>
                            <div class="col-sm-7">
                                <input id="sort" value="$store.companyPhone" name="sort" class="form-control" type="text" disabled/>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">注册总资金</label>
                            <div class="col-sm-7">
                                <input value="$store.companyRegisteredCapital" class="form-control" type="text" disabled/>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">联系人姓名</label>
                            <div class="col-sm-7">
                                <input value="$store.contactsName" name="sort" class="form-control" type="text" disabled/>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">联系人电话</label>
                            <div class="col-sm-7">
                                <input value="$store.contactsPhone"class="form-control" type="text" disabled/>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">营业执照号</label>
                            <div class="col-sm-7">
                                <input value="$store.businessLicenceNumber" class="form-control" type="text" disabled/>
                            </div>
                        </div>
                        
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">法定经营范围</label>
                            <div class="col-sm-7">
                                <input value="$store.businessSphere" class="form-control" type="text" disabled/>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">营业执照电子版</label>
                            <div class="col-sm-3">
                                <img src="$store.businessLicenceNumberElectronic"/>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">税务登记证号</label>
                            <div class="col-sm-7">
                                <input type="text" class="form-control" value="$store.taxRegistrationCertificateElectronic" disabled>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">店铺分类名称</label>
                            <div class="col-sm-7">
                                <input type="text" class="form-control" value="$store.scName" disabled>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">付款金额</label>
                            <div class="col-sm-7">
                                <input type="text" class="form-control" value="$store.payingAmount" disabled>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
