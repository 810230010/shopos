#parse("/store/models/begin.vm")

#parse("/store/models/header.vm")
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>订单-退换货详情</h2>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox">
                <div class="ibox-title">
                    <h5>订单详情</h5>
                    <p align="right">
                        <a type="button" class="btn btn-w-m btn-primary btn-small" href="/store/refund/successRefundList?type=''">返回</a>
                    </p>
                </div>
                <div class="ibox-content">
                    <form class="form-horizontal">
                        <div class="form-group">
                            <label class="col-sm-3 control-label">订单编号</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.orderSn)
                                    $info.orderSn
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">买家会员名</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.memberName)
                                    $info.memberName
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">商品名称</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.goodsName)
                                    $info.goodsName
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">商品数量</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.goodsNum)
                                    $info.goodsNum
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">退款金额</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.refundAmount)
                                    $info.refundAmount
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">退换货原因</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.reasonInfo)
                                    $info.reasonInfo
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">卖家备注</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.sellerMessage)
                                    $info.sellerMessage
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">管理员备注</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.adminMessage)
                                    $info.adminMessage
                                    #end
                                </span>
                            </div>
                        </div>

                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">物流单号</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.invoiceNo)
                                    $info.invoiceNo
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">发货时间</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.shipTime)
                                    $info.shipTime
                                    #end
                                </span>
                            </div>
                        </div>
                        <div class="hr-line-dashed"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label">收货时间</label>
                            <div class="col-sm-7">
                                <span class="form-control">
                                    #if($info.receiveTime)
                                    $info.receiveTime
                                    #end
                                </span>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
#parse("/admin/models/footer.vm")
#parse("/admin/models/end.vm")